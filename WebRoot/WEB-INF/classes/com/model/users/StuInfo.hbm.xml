<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class catalog="examol" name="com.model.users.StuInfo" table="stuinfo">
  <id column="StuID" name="stuid" type="long">
   <generator class="native"/>
  </id>
  <property column="StuName" generated="never" lazy="false"
   name="stuname" type="string"/>
  <property column="StuPwd" generated="never" lazy="false" name="stupwd" type="string"/>
  <property column="Grade" generated="never" lazy="false" name="grade" type="string"/>
  <property column="Major" generated="never" lazy="false" name="major" type="string"/>
  <property column="Class" generated="never" lazy="false" name="classes" type="string"/>
  <property column="JoinYear" generated="never" lazy="false"
   name="joinyear" type="string"/>
 </class>
</hibernate-mapping>
